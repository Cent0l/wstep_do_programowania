#include <iostream>
#include <vector>

using namespace std;

void wypisz(vector<int>tab)
{
	for (int i = 0; i < tab.size(); i++)
	{
		cout << tab[i] << endl;
	}
}

int max(vector<int>tab)
{
	int wynik = tab[0];
	for (int i = 1; i < tab.size(); i++)
	{
		if (tab[i] > wynik)
			wynik = tab[i];

	}
	return wynik;
}
bool nalezy(vector<int>tab,int element)
{
	bool wynik = false;	
for (int i = 0; i < tab.size(); i++)
{
	if (tab[i] == element)
	{
		wynik = true;
		break;
	}
}
return wynik;
}


int main()
{
	vector<int>tab = {8,1,2,3,8,5};
	wypisz(tab);
	cout << endl;
	cout << nalezy(tab,3);

}
