#include <iostream>
#include <vector>

using namespace std;

void wypisz(vector<int>tab)
{
	for (int i = 0; i < tab.size(); i++)
	{
		cout << tab[i] << endl;
	}
}

int max(vector<int>tab)
{
	int wynik = tab[0];
	for (int i = 1; i < tab.size(); i++)
	{
		if (tab[i] > wynik)
			wynik = tab[i];

	}
	return wynik;
}

